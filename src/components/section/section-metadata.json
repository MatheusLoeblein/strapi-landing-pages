{
  "collectionName": "components_section_section_metadata",
  "info": {
    "displayName": "section_metadata",
    "icon": "dashboard"
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "minLength": 3,
      "unique": true
    },
    "section_id": {
      "type": "string",
      "regex": "^[a-zA-Z]+([0-9-]*[a-zA-Z]+)*$",
      "minLength": 3,
      "maxLength": 255,
      "required": true
    },
    "background": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
